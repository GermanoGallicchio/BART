        <Param val="" valType="extendedCode" updates="constant" name="End JS Routine"/>
        <Param val="# earnings for this balloon (trial)&amp;#10;if popped==False:&amp;#10;    earnings_thisBalloon = np.round(nPumps*pumpWorth,2)&amp;#10;    if trials.thisN==0:&amp;#10;        bankedPumps = nPumps&amp;#10;    else:&amp;#10;        bankedPumps = bankedPumps + nPumps &amp;#10;    &amp;#10;else:&amp;#10;    earnings_thisBalloon = 0&amp;#10;&amp;#10;&amp;#10;# cumulative earnings so far across all balloons (trials)&amp;#10;earnings = np.round(bankedPumps*pumpWorth, 2)&amp;#10;&amp;#10;&amp;#10;#save data&amp;#10;trials.addData('nPumps_wouldPop', popPump)&amp;#10;trials.addData('nPumps_done', nPumps)&amp;#10;trials.addData('popped', popped)&amp;#10;trials.addData('earnings_thisBalloon', earnings_thisBalloon)&amp;#10;trials.addData('bankedPumps', bankedPumps)&amp;#10;trials.addData('earnings', earnings)&amp;#10;#trials.addData('earnings', thisBalloonEarnings)&amp;#10;trials.addData('xSize', xSize)&amp;#10;trials.addData('ySize', ySize)&amp;#10;&amp;#10;&amp;#10;&amp;#10;&amp;#10;&amp;#10;earnings_previousBalloon = earnings_thisBalloon&amp;#10;&amp;#10;" valType="extendedCode" updates="constant" name="End Routine"/>
